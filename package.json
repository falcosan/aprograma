{
  "name": "aprograma",
  "version": "3.0.0",
  "description": "Aprograma",
  "contributors": [
    "Daniele Falchetti <danielefalche@gmail.com>",
    "Aprograma"
  ],
  "private": true,
  "scripts": {
    "push:dev": "git checkout develop && git add -A && git commit -m 'develop' && git push origin develop",
    "push:prod": "git checkout develop && git fetch origin master && git merge master && git push origin develop:master && git fetch origin master:master",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js --fix",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxtjs/robots": "^3.0.0",
    "@pinia/nuxt": "^0.4.11",
    "@storyblok/nuxt": "^5.5.2",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "core-js": "^3.31.1",
    "highlight.js": "^11.8.0",
    "marked": "^5.1.1",
    "nuxt": "^3.6.2"
  },
  "devDependencies": {
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxtjs/device": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vite-pwa/nuxt": "^0.1.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nuxt-security": "^0.13.1",
    "prettier": "^3.0.0",
    "rss": "^1.2.2",
    "sitemap": "^7.1.1",
    "typescript": "^5.1.6"
  },
  "main": ".eslintrc.cjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falcosan/aprograma_nuxt.git"
  },
