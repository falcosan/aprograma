{
  "name": "aprograma",
  "version": "3.0.0",
  "description": "Aprograma",
  "contributors": [
    "Daniele Falchetti <danielefalche@gmail.com>",
    "Aprograma"
  ],
  "private": true,
  "scripts": {
    "push:dev": "git checkout develop && git add -A && git commit -m 'develop' && git push origin develop",
    "push:prod": "git checkout develop && git fetch origin master && git merge master && git push origin develop:master && git fetch origin master:master",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js --fix",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@storyblok/nuxt": "^5.7.7",
    "@vueuse/core": "^10.6.1",
    "highlight.js": "^11.9.0",
    "marked": "^10.0.0",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@nuxt/image": "^1.0.0",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/device": "^3.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^8.0.0-rc.5",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/tailwindcss": "^6.9.5",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@vite-pwa/nuxt": "^0.2.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nuxt": "3.8.1",
    "nuxt-security": "1.0.0-rc.4",
    "rss": "^1.2.2",
    "sitemap": "^7.1.1",
    "typescript": "^5.2.2",
    "vue-gtag-next": "^1.14.0"
  },
  "main": ".eslintrc.cjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falcosan/aprograma_nuxt.git"
  },
